{
  "version": 2,
  "builds": [
    { "src": "./backend/api/index.js", "use": "@vercel/node"},
    { "src": "./frontend/src/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "./backend/api/index.js" },
    { "src": "/(.*)", "dest": "./frontend/src/index.js" }
  ]
}
